{% extends 'base.html.twig' %}

{% block title %}ProfilClient index{% endblock %}

{% block body %}
    <h1>ProfilClient index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>NumeroProfilCompte</th>
                <th>IsActif</th>
                <th>CreatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for profil_client in profil_clients %}
            <tr>
                <td>{{ profil_client.id }}</td>
                <td>{{ profil_client.numeroProfilCompte }}</td>
                <td>{{ profil_client.isActif ? 'Yes' : 'No' }}</td>
                <td>{{ profil_client.createdAt ? profil_client.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_profil_client_show', {'id': profil_client.id}) }}">show</a>
                    <a href="{{ path('app_profil_client_edit', {'id': profil_client.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_profil_client_new') }}">Create new</a>
{% endblock %}
